\outl0\strokewidth0 \strokec2 INGREDIENTS\
\pard\pardeftab720\sl380\sa360

\fs32 \cf4 \expnd0\expndtw0\kerning0
\outl0\strokewidth0 \strokec4 1 large, round loaf of uncut sourdough bread\
1/2 cup (1 stick) cold salted butter, cut into 16 slices\
1 cup shredded white cheddar cheese\
1 cup shredded gouda\
1 tablespoon minced rosemary\
1 tablespoon minced sage\
2 garlic cloves, minced\
1/4 cup pomegranate seeds\
Cracked black pepper to taste\
Reynolds Wrap
\fs24 \expnd0\expndtw0\kerning0
\outl0\strokewidth0 \'ae
\fs32 \expnd0\expndtw0\kerning0
